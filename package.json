  "engines": {
    "node": "10.15.2"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "nodemon --ignore json/ --exec babel-node src/server.js",
    "server": "nodemon --ignore json/ --exec babel-node src/server.js",
